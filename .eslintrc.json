{
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
	"globals": {},
	"parserOptions": {
		"sourceType": "module"
	},
	"extends": ["eslint:recommended", "prettier"],
	"rules": {
		"no-alert": "warn",
		"no-console": "warn",
		"no-unused-vars": "warn",
		"no-var": "warn",
		"eqeqeq": "warn"
	}
}
